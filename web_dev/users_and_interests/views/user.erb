<h2>Details for <%= @user_name.capitalize %>:</h2>

<h4>Email address:</h4>
  <p><%= @email %></p>

<h4>Interests:</h4>
<p><%= @interests.join(", ") %></p>

<ul>
  <% @users.each do |name, _| %>
    <% unless name == @user_name %>
      <li><a href="/users/<%= name %>"><%= name.capitalize %><a></li>
    <% end %>
  <% end %>
</ul>
